{
    "name": "interface-to-json-schema",
    "version": "1.0.0",
    "author": "Matthew Duong <matt@timeweave.com.au>",
    "license": "MIT",
    "dependencies": {
        "@types/node": "^14.0.14",
        "prettier": "2.0.5",
        "ts-node": "^8.10.2",
        "typescript": "^4.0.5"
    },
    "scripts": {
        "generate-validation-enum": "node ./.scripts/generate-validation-enum.js",
        "generate-typecheck": "typescript-json-schema tsconfig.json * --out src/validation.schema.json --required --noExtraProps --topRef",
        "start": "yarn run generate-typecheck && tsc && node dist/Startup.js",
        "prettier": "prettier --write \"{src,web,.scripts}/**/*.{js,jsx,ts,tsx}\" \"*.{js,json,md,yml,yaml}\""
    },
    "devDependencies": {
        "typescript-json-schema": "^0.43.0"
    }
}
